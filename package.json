{
  "name": "@zhuangya/heap",
  "author": "Ya Zhuang <zhuangya@users.noreply.github.com> (https://zhuangya.me/)",
  "license": "MIT",
  "version": "0.1.0",
  "description": "heap in ts",
  "main": "dist/index.js",
  "devDependencies": {
    "esbuild": "^0.11.5",
    "esbuild-jest": "^0.5.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "scripts": {
    "precommit": "lint-staged",
